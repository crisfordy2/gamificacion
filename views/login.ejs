<html lang="es">
  <head>
    <title>Elements - Road Trip by TEMPLATED</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" href="/assets/css/main.css" />
  </head>
  <body class="subpage" style="background-color: black">
    <!-- Header -->
    <header id="header" class="alt">
      <div class="logo">
        <a href="/home"> Gamificación <span>DevOps</span></a>
      </div>
      <a href="#menu"><span>Menu</span></a>
    </header>

    <!-- Nav -->
    <nav id="menu">
      <ul class="links">
        <li><a href="/home">Home</a></li>
        <li><a href="/home/login">Login</a></li>
        <li><a href="/home/contacto">contactenos</a></li>
      </ul>
    </nav>

    <!-- Main -->
    <div id="main" class="container">
      <footer id="footer" style="background-color: gray">
        <div class="inner">
          <h1 style="text-align: center; font-size: 25px">Login</h1>

          <form action="/home/loginUser" method="post">
            <br />
            <label for="userUser">Usuario:</label>
            <input type="text" name="userUser" placeholder="Usuario" />
            <br />
            <label for="passUser">Contraseña:</label>
            <input type="password" name="passUser" placeholder="Contraseña" minlength="4" />
            <br />

            <button type="submit">Iniciar sesion</button>
          </form>

          <a href="/home/registro"><button>Registrarse</button></a>
        </div>
      </footer>
    </div>

    <pre style="background-color: white; color: black">
				<%= mensaje %>
			</pre
    >

    <!-- Scripts -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery.scrolly@0.0.1/jquery.scrolly.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.scrollex/0.2.1/jquery.scrollex.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/skel/3.0.1/skel.min.js"></script>
    <script src="/assets/js/util.js"></script>
    <script src="/assets/js/main.js"></script>
  </body>
</html>
